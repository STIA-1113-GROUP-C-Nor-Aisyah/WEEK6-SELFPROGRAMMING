package mySwitch;

import java.util.Scanner;

public class MyGrade {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner sc = new Scanner (System.in);
		
		System.out.println("Enter grade from A, B, C, D, E and F: ");
		char grade = sc.next().charAt(0);
		
		if (grade == 'A')
			System.out.println("Grade A mark more than 90");
		else if (grade == 'B')
			System.out.println("Grade B mark more than 80 and less than 90");
		else if (grade == 'C')
			System.out.println("Grade C mark more than 70 and less than 80");
		else if (grade == 'D')
			System.out.println("Grade D mark more than 60 and less than 70");
		else if (grade == 'E')
			System.out.println("Grade E mark more than 50 and less than 60");
		else if (grade == 'F')
			System.out.println("Grade A mark less than 40");
		else
	            System.out.println("Invalid grade");

		sc.close();
	}

}
